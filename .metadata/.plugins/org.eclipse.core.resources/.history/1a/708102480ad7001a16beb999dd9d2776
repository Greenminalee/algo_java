		for (int i = 0; i < n; i++) {
			result[i] = scan.next();
		}
		solution(beg);
	}
	
	static int solution(String beg)
	{
		for (int i = 0; i < result.length; i++) {
			
		}
		return (0);
	}
	
	static void search(int size, int index) {
		System.out.println(result[size].charAt(index));
	}
	

}
