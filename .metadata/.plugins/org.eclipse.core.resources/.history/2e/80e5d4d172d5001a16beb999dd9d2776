package algo;

/*
 * https://www.acmicpc.net/problem/5532
 */
import java.util.Scanner;

public class Algo15 {

	public static void main(String[] args) 
	{
		Scanner scan = new Scanner(System.in);
		int T;
		int N;
		N = scan.nextInt();
		int[][] stickers = new int[2][N];
		for (int t = 0; t < T; t++) 
		{
			for (int i = 0; i < 2; i++)
			{
				for(int j = 0; j < N; j++)
				{
					stickers[i][j] = scan.nextInt();
				}
			}
		}
		}
	
	static int solution()
	{
		return (0);
	}
}
