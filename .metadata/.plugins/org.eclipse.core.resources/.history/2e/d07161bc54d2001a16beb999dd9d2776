package algo;

import java.util.LinkedList;

public class Algo08 {

	public static void main(String[] args) {
		LinkedList<Integer> list = new LinkedList<Integer>();
		

	}
	
	static int solution(LinkedList<Integer> list, String str)
	{
		int  answer;
		if(str.equals("push_front"))
		{
			list.add(0,2);
		}
		
		if(str.equals("push_back"))
		{
			list.add(list.size(), 1);
		}
		
		if(str.equals("pop_front"))
		{
			if (list.size() != 0)
				(list.get(0));
			else
				return (-1);
		}
		
		if(str.equals("pop_back"))
		{
			list.get(list.size());
			list.remove(list.size());		
		}
		
		if(str.equals("size"))
		{
			list.add(list.size(), 1);
		}
		
		if(str.equals("empty"))
		{
			list.add(list.size(), 1);
		}
		
		
		if(str.equals("front"))
		{
			list.add(list.size(), 1);
		}
		
		if(str.equals("back"))
		{
			list.add(list.size(), 1);
		}
		
		return (0);
	}

}
