package algo;

import java.util.Scanner;

/*
 * https://www.acmicpc.net/problem/11123
 * 참고1 https://rebas.kr/808
 */
public class Algo23 {
	boolean[][] check = new boolean[100][100];
	int dx[] = {-1, 0, 1, 0}, dy[] = {0, 1, 0, -1};
	String [] grid = new String[H];
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		int H = scan.nextInt();
		int W = scan.nextInt();
		for (int i = 0; i < H; i++) {
			for (int j = 0; j < W; j++) {
				grid[i] = scan.next();
			}
		}
	}
	
	int dfs(int x, int y) {
		check[x][y] = true;
		for (int i = 0; i <4; i++) {
			int nx = x = dx[i], ny = y +dy[i];
			
		}
	}
	
	static int solution(String grid, int H, int W)
	{
		
		for (int i = 0; i < H; i++) {
			for (int j = 0; j < W; j++) {
				grid[H][W]
			}
		}
		return (0);
	}
}
