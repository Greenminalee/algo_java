package algo;

public class Algo01 {

	public static void main(String[] args) {
		int i;
		int j;
		
		i = 0;
		while (i < 5)
		{
			j = 5;
			while (j >= 0)
			{
				if(j >i )
					System.out.println(" ");
				else
					System.out.print("*");
				j++;
			}
			System.out.print("\n");
			i++;
				
		}
	}

}
