package algo;

import java.util.*;
public class Algo02 {

	public static void main(String[] args) {
		int i;
		int n;
		int k;
		LinkedList<Integer> list = new LinkedList<Integer>();

		i = 0;
		n = 7;
		k = 3;
		while (i < n)
		{
			list.add(i);
			i++;
		}
		i = 0;
		while (list.size() != 0)
		{
			i += k;
			System.out.println(list.get(i));
			list.remove(i);
		}
	}
}
