package algo;

import java.util.Scanner;

public class Algo40 {

	public static void main(String[] args) {
		solution();
	}

	static void solution() {
		Scanner scan = new Scanner(System.in);
		int p = scan.nextInt();
		int[][] size = new int[p][p];
		for(int i = 0; i < p; i++) {
			size[i][i]
		}
		//검은 도화지의 크
	}

}
