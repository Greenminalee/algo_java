package algo;

/*
 * https://www.acmicpc.net/problem/2908
 */
public class Algo10 {

	public static void main(String[] args) {
		
		System.out.println(solution(123, 456));
	}
	
	static int solution(int a, int b) 
	{
		String snum = Integer.toString(a));
		String tmp = "";
		
		for(int i = snum1.length() - 1; i >= 0; i--)
			tmp += snum1.charAt(i);
		a = Integer.parseInt(tmp);
		tmp = "";
		for(int i = snum2.length() - 1; i >= 0; i--)
			tmp += snum2.charAt(i);
		b = Integer.parseInt(tmp);
		
		a = a > b ? a : b; 
		return (a);
	}
}
