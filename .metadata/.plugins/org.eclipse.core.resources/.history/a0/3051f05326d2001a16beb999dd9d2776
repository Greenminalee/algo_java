public class Algo07 {

	public static void main(String[] args) {		
		
	}
	
	static int solution(String str)
	{
		String origin = "hello";
		String tmp = "hi";
		
		for(int i = 0; i <str.length(); i++)
		{
			if(str.charAt(i) == ' ')
				if(origin.equals(tmp))
				else
				{
					origin.replaceAll(origin, tmp);
					cnt++;
				}
		}
		return (0);
	}

}
