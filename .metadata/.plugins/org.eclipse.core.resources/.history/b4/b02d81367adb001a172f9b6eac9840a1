package algo;

import java.util.Scanner;

public class Algo40 {

	public static void main(String[] args) {
		solution();
	}

	static void solution() {
		Scanner scan = new Scanner(System.in);
		int p = scan.nextInt();
		int h, w;
		int cnt = 0;
		int[][] map = new int[100][100];
		for (int i = 0; i < p; i++) {
			w = scan.nextInt();
			h = scan.nextInt();
			for (int j = 0; j < 10; j++) {
				for
				map[h + j][w + j] = 1;
			}

		}
		for(int i = 0; i < 100; i++) {
			for(int j = 0; j < 100; j++) {
				System.out.print(map[i][j]);
			}
			System.out.println();
		}

		// 검은 도화지의 크
	}

}
