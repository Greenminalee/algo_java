package algo;

import com.sun.management.VMOption.Origin;

public class Algo07 {

	public static void main(String[] args) {		
		String a = "hello";
		String b = "hi";
		a = b;
		System.out.println(a);
		
	}
	
	static int solution(String str)
	{
		String origin = "";
		String tmp = "";
		int cnt = 0;
		
		for(int i = 0; i < str.length(); i++)
		{
			
			if(str.charAt(i) != ' ') 
			{
				tmp += str.charAt(i);
				
			}
			else
			{
				if(origin.equals(tmp))
				{
					
				}
				else
				{
					origin = tmp;
					cnt++;
				}
			}
			
		}
		return (0);
	}

}
