package algo;

/*
 * https://www.acmicpc.net/problem/2908
 */
public class Algo10 {

	public static void main(String[] args) {
		
		solution(123, 456);
	}
	
	static int solution(int a, int b) 
	{
		String snum1 = Integer.toString(a);
		String snum2 = Integer.toString(b);
		String tmp = "";
		
		for(int i = snum1.length() - 1; i >= 0; i--)
			tmp += snum1.charAt(i);
		a = Integer.parseInt(tmp);
		for(int i = snum2.length() - 1; i >= 0; i--)
			tmp += snum2.charAt(i);
		tmp = snum2;
		
		System.out.println(tmp);
		return (0);
	}
}
