package algo;

import java.util.Scanner;

public class Algo16 {

	public static void main(String[] args) {

		solution();
	}
	
	static int solution()
	{
		Scanner scan = new Scanner(System.in);
		int L, A, B, C, D;
		int n1, n2;
		
		L = scan.nextInt();
		A = scan.nextInt();
		B = scan.nextInt();
		C = scan.nextInt();
		D = scan.nextInt();
		n1 = A / C;
		n2 = B / D;
		if (A % C > 0)
			n1 += 1;
		return (0);
	}
}