package algo;

import java.util.Scanner;

/*
 * https://www.acmicpc.net/problem/2953
 */
public class Algo36 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	
	static void sloution() {
		Scanner scan = new Scanner(System.in); 
		int sum = 0;
		int[] cooker = new int[2];
		for(int i = 0; i < 5; i++) {
			for(int j = 0; j < 5; j++) {
				sum += scan.nextInt();
			}
			if (sum > cooker[i])
				cooker[i] = sum;
		}
		
	}

}
