		N = scan.nextInt();
	}

	static int solution(int N) {
		Scanner scan = new Scanner(System.in);
		int sum1 = 0, sum2 = 0;
		int[][] stickers = new int[2][N];
		for (int i = 0; i < 2; i++) {
			for (int j = 0; j < N; j++) {
				stickers[i][j] = scan.nextInt();
			}
		}

		for(int i = 0; i < N; i += 2)
		{
			for(int j = 1; j < N; j += 2)
			{
				
			}	
		}
		return (0);
	}
}
