package algo;

import java.util.LinkedList;
import java.util.Scanner;

public class Algo33 {

	public static void main(String[] args) {

	}

	static void solution() {
		Scanner scan = new Scanner(System.in);
		int n1 = scan.nextInt(), n2 = scan.nextInt();
		Ant[] ant1 = new Ant[4];
		Ant[] ant2 = new Ant[4];
		for (int i = 0; i < n1; i++) {
			ant1[i].name = scan.next().charAt(0);
			ant1[i].group = 'A';
		}
		for (int i = 0; i < n1; i++) {
			ant2[i].name = scan.next().charAt(0);
			ant2[i].group = 'B';
		}
	}

}

class Ant {
	char name;
	char group;
}
